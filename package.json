{
  "name": "read-quest",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "start": "npx expo start -c",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "test": "jest --watchAll",
    "generate-themes": "tamagui generate-themes ./theme-builder.ts ./theme-output.ts",
    "check-types": "tsc",
    "format": "yarn prettier --write",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "clean": "rm -rf node_modules && rm -rf packages/**/node_modules && rm -rf apps/**/node_modules"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@react-navigation/native": "^6.1.17",
    "@tamagui/cli": "^1.102.3",
    "@tamagui/config": "^1.102.1",
    "@tamagui/lucide-icons": "^1.102.1",
    "@tamagui/toast": "^1.102.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-expo": "^11.0.6",
    "burnt": "^0.12.2",
    "drizzle-orm": "^0.32.0",
    "expo": "~51.0.9",
    "expo-font": "~12.0.6",
    "expo-image-picker": "~15.0.7",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.15",
    "expo-splash-screen": "~0.27.4",
    "expo-sqlite": "^14.0.4",
    "expo-status-bar": "^1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "nanoid": "^5.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-native": "0.74.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.12",
    "react-query": "^3.39.3",
    "tamagui": "^1.102.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~0.18.4",
    "@expo/metro-runtime": "~3.2.1",
    "@tamagui/babel-plugin": "^1.102.1",
    "@tamagui/metro-plugin": "^1.102.1",
    "@types/react": "~18.2.79",
    "@types/react-native": "^0.73.0",
    "drizzle-kit": "^0.23.0",
    "typescript": "^5.5.2"
  },
  "private": true,
  "packageManager": "yarn@4.0.2"
}
